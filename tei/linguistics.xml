<TEI xmlns="http://www.tei-c.org/ns/1.0">
  <teiHeader>
      <fileDesc>
         <titleStmt>
            <title>TEI for linguists</title>
            <author>Peter Stadler</author>
         </titleStmt>
         <publicationStmt>
            <p>freely available</p>
         </publicationStmt>
